{
  "name": "sushi-go-friend",
  "author": "Sean McPherson",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://seanmcp.github.io/sushi-go-friend",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-feather": "^1.1.5",
    "react-scripts": "2.1.1",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier 'src/**/*.{js,jsx}' --write",
    "fix:dependencies": "rm -rf node_modules/ yarn.lock && yarn",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1"
  }
}
