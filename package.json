  "private": true,
  "dependencies": {
    "prettier": "^1.15.3",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-feather": "^1.1.5",
    "react-scripts": "2.1.1",
    "styled-components": "^4.1.3"
  },
  "scripts": {
